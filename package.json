{
  "name": "preact-cli-plugin-styled-components",
  "version": "1.0.0",
  "description": "Adds the styled-components Babel plugin to Preact CLI",
  "license": "MIT",
  "author": "Robin van der Vleuten <robin@webstronauts.co>",
  "main": "index.js",
  "keywords": [
    "preact",
    "plugin",
    "styled-components"
  ],
  "scripts": {
    "test": "xo"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": true,
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  },
  "repository": "robinvdvleuten/preact-cli-plugin-styled-components",
  "bugs": {
    "url": "https://github.com/robinvdvleuten/preact-cli-plugin-styled-components/issues"
  },
  "homepage": "https://github.com/robinvdvleuten/preact-cli-plugin-styled-components#readme",
  "dependencies": {
    "babel-plugin-styled-components": "^1.5.1"
  },
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "peerDependencies": {
    "styled-components": "^3.2.0"
  }
}
